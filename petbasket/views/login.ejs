<!DOCTYPE html>

    <head>
        <% include head %>
        <title>Log In</title>

    </head>
    <body>
    <% include navbar %>
     <main>
        <div class="container">
				<div class="row">
					<h3 class="center-align">Log In</h3>
					<form class="offset-s2 col s8 card darken-1" action="/loginProcess" method="post">
						<div class="row">
							<div class="col s12">
								<i class="material-icons email-icon">email</i>
							</div>
							<div class="input-field col s12">
								<input id="email" type="email" class="validate" name="email">
								<label for="email">Email</label>
							</div>
						</div>
						<div class="row">
							<div class="input-field col s12">
								<input id="password" type="password" class="validate" name="password">
								<label for="password">Password</label>
							</div>
						</div>
						<div class="row">
							<div class="input-field col s12">
								<div class="center-align">
									<button class="btn btn-block waves-effect waves-light" type="submit" name="log-in">Log Me In!
										<i class="material-icons right">send</i>
									</button>
								</div>
							</div>
						</div>
					</form>
				</div>
			</div>
	    </main>
        <% include footer %>
    </body>
</html>